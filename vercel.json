{
    "builds": [
      {
        "src": "website/website/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.10" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "website/website/wsgi.py"
      },
      {
        "src": "/website/static/(.*)",
        "dest": "/static/$1"
      }
    ]
  }