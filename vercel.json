{
    "builds": [
      {
        "src": "website/website/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb"}
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/website/website/wsgi.py"
      },
      {
        "src": "/website/static/(.*)",
        "dest": "/static/$1"
      }
    ]
  }